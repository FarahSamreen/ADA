
#include <stdio.h>

int main()
{
    int arr[5];
     int i,j;
    for(j=0;j<5;j++){
        printf("Enter elements of the array:");
        scanf("%d",&arr[j]);
    }
    printf("Enter element to search:");
    scanf("%d",&i);
    for(j=0;j<5;j++){
        if(arr[j]==i){
            printf("Element found");
            return 0;
        }
    }
    printf("Not found");
    return 0;
}
